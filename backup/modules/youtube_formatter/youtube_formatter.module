<?php

/**
 * @file
 * Defines simple YouTube formatter for the link field.
 */

/**
 * Implements hook_theme().
 */
function youtube_formatter_theme() {
  return array(
    'youtube_link_formatter' => array(
      'variables' => array('url' => NULL),
      'template' => 'youtube-link-formatter',
    ),
  );
}